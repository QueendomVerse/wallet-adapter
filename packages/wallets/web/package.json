{
    "name": "@mindblox-wallet-adapter/web",
    "version": "0.0.1",
    "author": "Phillip Strefling <phillip@chiefmetaverse.co>",
    "repository": "https://github.com/QueendomVerse/wallet-adapter",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib",
        "src",
        "LICENSE"
    ],
    "engines": {
        "node": ">=16"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/types/index.d.ts",
    "exports": {
        "require": "./lib/cjs/index.js",
        "import": "./lib/esm/index.js",
        "types": "./lib/types/index.d.ts"
    },
    "scripts": {
        "build": "tsc --build --verbose && pnpm run package",
        "clean": "shx mkdir -p lib && rimraf lib",
        "lint": "prettier --check 'src/{*,**/*}.{ts,tsx,js,jsx,json}' && eslint",
        "package": "shx mkdir -p lib/cjs && shx echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json"
    },
    "dependencies": {
        "@ant-design/icons": "^5.1.4",
        "@mindblox-wallet-adapter/base": "workspace:^",
        "@mindblox-wallet-adapter/near": "workspace:^",
        "@mindblox-wallet-adapter/networks": "workspace:^",
        "@mindblox-wallet-adapter/phantom": "workspace:^",
        "@mindblox-wallet-adapter/react": "workspace:^",
        "@mindblox-wallet-adapter/solana": "workspace:^",
        "@mindblox-wallet-adapter/unsafe-burner": "workspace:^",
        "@paralleldrive/cuid2": "^2.2.1",
        "@reduxjs/toolkit": "^1.9.5",
        "bcryptjs": "^2.4.3",
        "bip39": "^3.1.0",
        "bs58": "^5.0.0",
        "dexie": "^3.2.4",
        "encoding": "^0.1.13",
        "eventemitter3": "^4.0.7",
        "formdata-node": "^5.0.1",
        "near-api-js": "^2.1.4",
        "node-fetch": "^2.6.7",
        "react": "^18.2.0",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "simple-crypto-js": "^3.0.1",
        "tweetnacl": "^1.0.3",
        "use-between": "^1.3.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7"
    }
}