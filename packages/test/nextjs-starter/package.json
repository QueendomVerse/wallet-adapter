        "lint": "prettier --check 'src/{*,**/*}.{ts,tsx,js,jsx,json}' && eslint && next lint"
    },
    "dependencies": {
        "@mindblox-wallet-adapter/base": "workspace:^",
        "@mindblox-wallet-adapter/react": "workspace:^",
        "@mindblox-wallet-adapter/react-ui": "workspace:^",
        "@mindblox-wallet-adapter/wallets": "workspace:^",
        "@mindblox-wallet-adapter/networks": "workspace:^",
        "@mindblox-wallet-adapter/solana": "workspace:^",
        "@mindblox-wallet-adapter/web": "workspace:^",
        "next": "^12.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node-fetch": "^2.6.4",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.6",
        "eslint": "8.22.0",
        "eslint-config-next": "^12.3.4",
        "prettier": "^3.0.0",
        "shx": "^0.3.4",
        "typescript": "~4.7.4"
    }
