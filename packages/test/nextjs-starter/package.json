{
    "name": "@mindblox-wallet-adapter/nextjs-starter",
    "version": "0.8.19",
    "author": "Phillip Strefling <phillip@queendom.io>",
    "repository": "https://github.com/QueendomVerse/wallet-adapter",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "public",
        "src",
        ".editorconfig",
        ".eslintrc.json",
        ".gitignore",
        ".prettierignore",
        ".prettierrc",
        "LICENSE",
        "next.config.js",
        "next-env.d.ts",
        "package.json",
        "README.md",
        "tsconfig.json"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "build": "tsc --build --verbose && next build",
        "clean": "rimraf .next .swc lib",
